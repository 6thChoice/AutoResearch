# Claude Code Container Environment Variables
# Copy this file to .env and fill in your values

# API Base URL (use Anthropic's default or a custom endpoint)
ANTHROPIC_BASE_URL=https://api.anthropic.com

# Required: Your Anthropic Auth Token
ANTHROPIC_AUTH_TOKEN=your-auth-token-here

# Model to use
ANTHROPIC_MODEL=claude-sonnet-4-6

# Optional: Timezone
TZ=UTC
